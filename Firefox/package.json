    {
      "description": "Width of panel in pixels",
      "type": "integer",
      "name": "width",
      "value": 660,
      "title": "Panel width:"
    },
    {
      "description": "Height of panel in pixels",
      "type": "integer",
      "name": "height",
      "value": 700,
      "title": "Panel height:"
    },
    {
      "title": "Display FAQs page on upgrades",
      "type": "bool",
      "name": "welcome",
      "value": true
